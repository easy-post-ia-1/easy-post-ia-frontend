{
  "name": "easy-post-ia-frontend",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "staging": "vite --mode staging",
    "dev": "vite --mode dev",
    "prod": "vite --mode prod",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lintox": "npx oxlint -D correctness",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc.json",
    "buildIOS": "vite build && npx cap sync && npx cap open ios && npx cap run ios",
    "buildAndroid": "vite build && npx cap sync && npx cap open android && npx cap run android",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:all": "vitest run",
    "test:integration": "vitest run --dir __tests__/e2e",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.15",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.0.2",
    "@mui/x-date-pickers": "^7.22.3",
    "@tanstack/react-query": "^5.59.15",
    "axios": "^1.7.7",
    "i18next": "^23.16.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "luxon": "^3.5.0",
    "notistack": "^3.0.1",
    "puppeteer": "^23.8.0",
    "puppeteer-core": "^23.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "react-router-dom": "^6.26.2",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0",
    "zustand": "^5.0.1",
    "zustymiddleware": "^1.2.0",
    "zustymiddlewarets": "^1.4.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@eslint/js": "^9.10.0",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@tanstack/react-query-devtools": "^5.59.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "oxlint": "^0.10.3",
    "prettier": "^3.3.3",
    "translation-check": "^1.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.6",
    "vite-plugin-oxlint": "^1.1.0",
    "vitest": "^2.1.1"
  }
}
